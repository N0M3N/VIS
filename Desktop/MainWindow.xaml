<Window x:Class="Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="VIS - Stavebni denik a dochazka">

    <DockPanel LastChildFill="True">
        <DockPanel LastChildFill="False" DockPanel.Dock="Top" Style="{StaticResource NavBar}">
            <TextBlock Text="Stavebni denik a dochazka" Style="{StaticResource Logo}" />
        </DockPanel>

        <Frame DockPanel.Dock="Top" x:Name="Frame" Background="{StaticResource Accent1}" NavigationUIVisibility="Hidden" />
    </DockPanel>
</Window>
